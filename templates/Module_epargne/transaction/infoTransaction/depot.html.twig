{% extends "base.html.twig" %}

{% block title %}
    Depot de compte epargne 
{% endblock %}

{% block head %}
    
{% endblock %}

{% block body %}

    <div class="modal" tabindex="-1" id="myModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header titre_modal">
                    <h5 class="modal-title text-center">Depot compte epargne </h5>
                </div>

                {{ form_start(form) }}
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-10 col-sm-10 m-auto">
                                {{ form_row(form.code)}}
                            </div>
                        </div>

                    </div>

                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary btn-sm"> Suivant </button>
                    </div>
                {{ form_end(form)}}
            </div>
        </div>
    </div>

{% endblock %}