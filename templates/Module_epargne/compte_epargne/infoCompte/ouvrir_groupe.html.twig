{% extends "base.html.twig" %}

{% block title %}
    Ouvrir un compte epargne groupe
{% endblock %}
{% block head 'compte epargne>Ouvrir une compte groupe' %}

{% block body %}
    <div class="modal fade" id="myModal">
        <div class="modal-dialog">
            <div class="modal-content">
        
                    <!-- Modal Header -->
                <div class="modal-header text-center titre_modal">
                    <h5 class="modal-title">Compte epargne client Groupe </h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"> </button>
                </div>
                {{ form_start(form) }} 
                    <!-- Modal body -->
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-8 col-sm-8 m-auto">
                                {{ form_row(form.code)}}
                            </div>
                        </div>
                    </div>
            
                    <!-- Modal footer -->
                    <div class="modal-footer mb-0">
                        <button type="submit" class="btn btn-primary btn-sm"> Suivant </button>
                    </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}