{% extends "base.html.twig" %}

{% block title 'Sortie compte epargne' %}
{% block head 'Sortie compte epargne' %}

{% block body %}

    <div class="modal" tabindex="-1" id="myModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header titre_modal">
                    <h5 class="modal-title text-center">Sortie compte epargne </h5>
                </div>

                {{ form_start(form) }}
                    <div class="modal-body"> 
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                {{ form_row(form.code)}}
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <p class="fw-bolder">Code  : <span id="codeclient"></span></p>
                                <p class="fw-bolder">Compte epargne  : <span id="compte_epargne"></span></p>
                                <p class="fw-bolder">Nom  : <span id="nom"></span></p>
                            </div>
                        </div>
                    </div>

                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary btn-sm"> Suivant </button>
                    </div>
                {{ form_end(form)}}
            </div>
        </div>
    </div>

{% endblock %}