{% extends "base.html.twig" %}

{% block title %}
        Rapport compte epargne
{% endblock %}
{% block head %}
{% endblock %}

{% block body %}
    <div class="modal fade" id="myModal">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
    
                <!-- Modal Header -->
                <div class="modal-header titre_modal">
                <h5 class="modal-title text-center"> Rapport compte epargne  </h5>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                {{ form_start(form) }} 
                <!-- Modal body -->
                    <div class="modal-body">    
                    <div class="row">
                        <div class="col-md-8 col-sm-8 col-xl-8 m-auto">
                            {{ form_row(form.type ) }} 
                            {{ form_row(form.individuel) }} 
                            {{ form_row(form.groupe) }} 
                            {{ form_row(form.agence) }}
                            {# <h6 class="text-center"> Date </h6>
                            {{ form_row(form.datearrete) }}  #}
                        </div>
                        {# <h6 class="text-center">Entre deux periode </h6>
                        <div class="row">
                            <div class="col-sm-6 col-md-6 col-xl-6">
                                {{ form_row(form.datedebut) }} 
                            </div>
                            <div class="col-sm-6 col-md-6 col-xl-6">
                                {{ form_row(form.datefin) }} 
                            </div>
                        </div>  #}
                    </div>
                </div>
            
                <!-- Modal footer -->
                <div class="modal-footer mb-0">
                    <button type="submit" class="btn btn-sm btn-primary"> Filtrer </button>      
                    </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}