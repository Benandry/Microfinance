{% extends "base.html.twig" %}

{% block title %} Ajout Membre Groupe {% endblock %}
{% block head %} Client>Groupe>Ajout Membre  {% endblock %}

{% block body %}
    <div class="modal" id="myModal">
        <div class="modal-dialog">
            <div class="modal-content">
        
                    <!-- Modal Header -->
                <div class="modal-header titre_modal">
                    <h4 class="modal-title text-center">Ajout Membre Groupe </h4>
                </div>
                {{ form_start(form) }} 
                    <!-- Modal body -->
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-10 col-sm-10">

                                <div id="info_client">
                                    {{ form_row(form.nom)}}
                                </div>
                            </div>
                           
                        </div>

                    </div>
            
                    <!-- Modal footer -->
                    <div class="modal-footer mb-0">
                        <input type="submit" value="valider">
                    </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div> 

{% endblock %}

{% block javascript %}
{% endblock %}