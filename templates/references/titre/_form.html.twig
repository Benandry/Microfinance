<!-- The Modal -->
<div class="modal" id="myModal">
   <div class="modal-dialog">
         <div class="modal-content">
   
               <!-- Modal Header -->
            <div class="modal-header titre_modal">
               <h4 class="modal-title text-center"> Titre </h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            {{ form_start(form) }} 
               <!-- Modal body -->
               <div class="modal-body">
                   {{ form_widget(form) }}
               </div>
         
               <!-- Modal footer -->
               <div class="modal-footer mb-0">
                     <button t class="btn btn-primary btn-sm">{{ button_label|default('Enregistrer') }}</button>
                     <a href="{{ path('app_titre_index') }}" class="btn btn-info btn-sm"> <i class="fa fa-eye"></i> Voir Liste</a>
               </div>
             {{ form_end(form) }}
         </div>
   </div>
</div>